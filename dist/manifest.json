{
  "name": "switchr",
  "version": "1.0",
  "description": "Extension toggler",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "switchr",
    "default_popup": "index.html"
  },
  "permissions": ["background", "management"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  }
}
